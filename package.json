        "@vue/eslint-config-typescript": "^13.0.0",
        "@prettier/plugin-php": "^0.22.2",
        "autoprefixer": "^10.4.12",
        "axios": "^1.11.0",
        "concurrently": "^9.0.1",
        "eslint": "^8",
        "eslint-plugin-vue": "^9.17.0",
